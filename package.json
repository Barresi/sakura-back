{
  "name": "sakura",
  "version": "0.1.0",
  "description": "Social network",
  "main": "app.ts",
  "scripts": {
    "dev": "npx nodemon source/app.ts | pino-pretty --translateTime",
    "dev:up": "docker compose -f docker/docker-compose.yml up -d --remove-orphans",
    "dev:down": "docker compose -f docker/docker-compose.yml down",
    "dev:db:migrate": "npx prisma migrate dev",
    "prepare": "husky install",
    "lint": "eslint \"./**/*.ts\" --quiet"
  },
  "prisma": {
    "schema": "source/prisma/schema.prisma"
  },
  "author": "Sakura dev team",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "config": "^3.3.9",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pino": "^8.11.0",
    "pino-http": "^8.4.0",
    "pino-pretty": "^9.3.0",
    "prettier": "^2.8.8",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@prisma/client": "^5.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/config": "^3.3.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^18.14.1",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.9",
    "@types/passport-local": "^1.0.35",
    "@types/redis": "^4.0.11",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "eslint": "^8.49.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
