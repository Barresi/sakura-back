    "@types/node": "^18.14.1",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.9",
    "@types/passport-local": "^1.0.35",
    "@types/redis": "^4.0.11",
    "husky": "^8.0.3",
    "nodemon": "^3.0.1",
    "prisma": "^5.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5"
  }
