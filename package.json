    "eslint-plugin-import": "^2.28.1",
    "husky": "^8.0.3",
    "nodemon": "^3.0.1",
    "prisma": "^5.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5"
  }
