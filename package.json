    "async-redis": "^2.0.0",
    "bcrypt": "^5.1.1",
    "config": "^3.3.9",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pino": "^8.11.0",
    "pino-http": "^8.4.0",
    "pino-pretty": "^9.3.0",
    "prettier": "^2.8.8",
    "redis": "^4.6.8",
    "socket.io": "^4.7.2",
    "zod": "^3.22.2"
  },
  "devDependencies": {
